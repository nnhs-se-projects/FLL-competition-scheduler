{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node Server",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["start"],
      "outputCapture": "std"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Node Client",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/assets",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug jrGeneticAlg.js",
      "program": "${workspaceFolder}/jrGeneticAlg.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug crossover.js",
      "program": "${workspaceFolder}/crossover.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug mergedSchedule.js",
      "program": "${workspaceFolder}/mergedSchedule.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug attempt.js",
      "program": "${workspaceFolder}/attempt.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug fullRandomScheduleTest.js",
      "program": "${workspaceFolder}/fullRandomScheduleTest.js"
    }
  ]
}
