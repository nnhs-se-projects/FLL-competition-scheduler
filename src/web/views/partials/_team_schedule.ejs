<div class="space-y-3">
  <% team.sort((a, b) => a.startTime - b.startTime).forEach(event => { %>
  <div class="bg-white rounded p-3 shadow-sm schedule-item">
    <div class="flex justify-between items-start">
      <div>
        <p class="font-medium">
          <%= event.type === 'tableRun' ? 'Robot Game' : event.type ===
          'robotJudging' ? 'Robot Design Judging' : 'Project Judging' %>
        </p>
        <p class="text-sm text-gray-500"><%= event.locationName %></p>
      </div>
      <div class="text-right">
        <p class="font-medium">
          <%= Math.floor(event.startTime/60) %>:<%=
          String(event.startTime%60).padStart(2, '0') %>
        </p>
        <p class="text-sm text-gray-500"><%= event.duration %> min</p>
      </div>
    </div>
  </div>
  <% }); %>
</div>
