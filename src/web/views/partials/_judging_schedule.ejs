<div class="overflow-x-auto">
  <table class="min-w-full divide-y divide-gray-200 print-table">
    <colgroup>
      <col style="width: 15%" />
      <col style="width: 10%" />
      <col style="width: 25%" />
      <col style="width: 30%" />
      <col style="width: 20%" />
    </colgroup>
    <thead class="bg-gray-50">
      <tr>
        <th
          class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
        >
          Time
        </th>
        <th
          class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
        >
          Team #
        </th>
        <th
          class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
        >
          Team Name
        </th>
        <th
          class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
        >
          Session Type
        </th>
        <th
          class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
        >
          Duration
        </th>
      </tr>
    </thead>
    <tbody class="bg-white divide-y divide-gray-200">
      <% room.sort((a, b) => a.startTime - b.startTime).forEach(event => { %>
      <tr class="hover:bg-gray-50 schedule-item">
        <td
          class="px-6 py-4 whitespace-nowrap text-sm text-gray-900 font-medium"
        >
          <%= Math.floor(event.startTime/60) %>:<%=
          String(event.startTime%60).padStart(2, '0') %>
        </td>
        <td
          class="px-6 py-4 whitespace-nowrap text-sm text-gray-900 font-medium"
        >
          <%= event.teamID || event.teamId || "-" %>
        </td>
        <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-900">
          <%= event.teamName || "N/A" %>
        </td>
        <td class="px-6 py-4 whitespace-nowrap text-sm">
          <% if (event.type === 'projectJudging') { %>
          <span class="text-green-600 font-medium">Project Judging</span>
          <% } else if (event.type === 'robotJudging') { %>
          <span class="text-purple-600 font-medium">Robot Design Judging</span>
          <% } else if (event.type === 'coreValues') { %>
          <span class="text-indigo-600 font-medium">Core Values Judging</span>
          <% } else if (event.type === 'judgeDeliberation') { %>
          <span class="text-gray-600 font-medium">Judge Deliberation</span>
          <% } else if (event.type === 'break') { %>
          <span class="text-gray-500 font-medium">Break</span>
          <% } else if (event.type === 'awards') { %>
          <span class="text-yellow-600 font-medium">Awards Prep</span>
          <% } else { %>
          <span class="font-medium"><%= event.type %></span>
          <% } %>
        </td>
        <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-900">
          <%= event.duration %> min
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
